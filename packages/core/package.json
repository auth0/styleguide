{
  "name": "@auth0/styleguide-core",
  "version": "3.1.0",
  "description": "Includes stylus variables, mixins and class utilities common to all Auth0 projects.",
  "main": "build/core.css",
  "style": "build/core.css",
  "scripts": {
    "prepublish": "NODE_ENV=production npm run build",
    "start": "gulp",
    "build": "gulp build",
    "publish:cdn": "gulp cdn && ccu",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint build/core.css",
    "test": "npm run lint:js"
  },
  "keywords": [
    "auth0",
    "styleguide",
    "css",
    "base"
  ],
  "author": "Auth0 (https://auth0.com)",
  "homepage": "https://styleguide.auth0.com/",
  "repository": "https://github.com/auth0/styleguide/tree/master/packages/core",
  "license": "UNLICENSED",
  "files": [
    "build",
    "src"
  ],
  "dependencies": {
    "normalize-css": "^2.3.1"
  },
  "devDependencies": {
    "@auth0/component-cdn-uploader": "^2.0.0",
    "bump-version": "^0.5.0",
    "eslint": "^3.19.0",
    "eslint-config-auth0-base": "^13.0.0",
    "eslint-plugin-import": "^2.3.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-load-plugins": "^1.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-stylus": "^2.5.0",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^15.0.1"
  },
  "ccu": {
    "name": "core",
    "cdn": "https://cdn.auth0.com",
    "mainBundleFile": "core.css",
    "remoteBasePath": "styleguide",
    "bucket": "assets.us.auth0.com",
    "localPath": "cdn"
  }
}
